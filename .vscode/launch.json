{
    "configurations": [
      {
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask":"Build: TestCore9.csproj",
        "name": "Launch ASP.NET Core App",
        "program": "${workspaceFolder}/bin/Debug/net9.0/TestCore9.dll",
        "args": [],
        "cwd": "${workspaceFolder}",
        "stopAtEntry": false,
        "launchBrowser": {
            "enabled": true
        },
        "serverReadyAction": {
          "action": "openExternally",
          "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
        },
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "sourceFileMap": {
          "/Views": "${workspaceFolder}/Views"
        }
      }
    ]
  }